package Domashno3;



public class Upr1 {
public static void main(String[] args) {
	

	String [][] menu=new String [][]{
			
			new String[] {"s1 ","s2 ","s3 "},
			new String[] {"o1 "," o2"," o3"," o4"," o5"," o6"},
			new String[] {"d1 ","d2 "},
			
			
	};
	
	
	for(int i = 0; i < menu.length; i++) {
		for (int j = 0; j < menu[i].length; j++) {
			if(j<menu[0].length){
				System.out.println( "Sypi:"+menu[i][j]);
			}
			else if(j<menu[1].length){
				System.out.println( "Osnovni:"+menu[i][j]);
			}
			else if(j<menu[2].length){	{
				System.out.println("Deserti:"+ menu[i][j]);
			}
				}
				System.out.println();
			}
			
			
			
	}
}
}
	
	
	
	



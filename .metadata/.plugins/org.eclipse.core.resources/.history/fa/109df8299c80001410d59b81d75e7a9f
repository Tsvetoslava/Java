import java.util.Scanner;


public class Numbers5 {
	
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int largest;
		int[] arr = new int[5];
		for (int i = 0; i <arr.length; i++) {
			System.out.println("Vuvedi "+(i+1)+" chislo");
			arr[i] = sc.nextInt();
	
		}
		
		
		 largest=arr[1];
        System.out.println("Chislata sa:");
        for (int i=0; i<arr.length; i++) {
             
              if (largest<arr[i]) 
            	  {
            	  largest=arr[i];
            	  }
             
        }
        
        
        System.out.println("Nai-golqmoto chislo e:"+largest);
	
	
}
}

import java.util.Scanner;


public class Cards {
	
	
	
		
		public static void main(String[] args) {
			
			String[] cards = {"A","K","Q","J","10","9","8","7","6","5","4","3","2"};
			String[] color = {"Clubs","Diamonds","Hearths","Spades"};
			
			
			for( int i = 0 ; i <= 3 ; i++ ){
				System.out.printf("%s\t" , color[i]);
			}
			
			for(int j = 0 ; j <= cards.length ; j++ ){
				System.out.println("");
				
				for( int k =1 ; k <= color.length ; k++ ){
					System.out.printf("%s \t" ,  cards[j]);
				}
			}
			
			}

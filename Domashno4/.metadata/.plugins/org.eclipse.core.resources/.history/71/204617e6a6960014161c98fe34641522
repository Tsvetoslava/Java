import java.util.Scanner;


public class Redica1 {
public static void main(String[] args) {
	
	int[] array= new int[10];
	Scanner sc=new Scanner(System.in);
	int counter=1;
	for (int i = 0; i < array.length; i++) {
		System.out.println("Vuvedi " +counter +" chislo: ");
		int number=sc.nextInt();
		array[i] = (number);
		counter++;
}
	for (int i : array) {
		System.out.print(i);
	}
	arr(array);
}


public static void arr(int[] array)
{
    int i = 0;
    int previusNum = array[0];
    int k = 0;
    boolean Flag = true;
    int[] temporaryArray = new int[array.length];
    int ElementCounter = 0;
    int OldElementCounter = 0;
    
   do {

        if (previusNum < array[i + 1])
        {
            k = i + 1;
            ElementCounter++;
            previusNum=array[i+1];

        }
        else if (array[i] >= array[i + 1])
        {
            previusNum = array[i + 1];
            if (ElementCounter > OldElementCounter)
            {
                for (int j = 0; j < ElementCounter+1; j++)
                {
                    temporaryArray[j] = array[k - ElementCounter];
                    k++;
                }
                OldElementCounter = ElementCounter;
            }
            ElementCounter = 0;

        }
        
        i++;
   }while(i<10);
    
    for (int z = 0; z < OldElementCounter+1; z++)
    {
        System.out.printf("%d ",temporaryArray[z]);
    }
}
}
import java.util.Scanner;


public class Redica1 {
public static void main(String[] args) {
	
	int[] array= new int[10];
	Scanner sc=new Scanner(System.in);
	int counter=1;
	for (int i = 0; i < array.length; i++) {
		System.out.println("Vuvedi " +counter +" chislo: ");
		int number=sc.nextInt();
		array[i] = (number);
		counter++;
}

	arr(array);
}


public static void arr(int[] array)
{
    int i = 0;
    int previusNum = array[0];
    int k = 0;
    boolean Flag = true;
    int[] temporaryArray = new int[array.length];
    int counter = 1;
    int oldCounter = 1;
    while (Flag)
    {

     
        if (array[i + 1]>=array[i])
        {
            previusNum = array[i + 1];
            if (counter >  oldCounter)
            {
                for (int j = 0; j <counter; j++)
                {
                    temporaryArray[j] = array[k - (counter-1)];
                    k++;
                }
                oldCounter = counter;
            }
            counter = 1;

        }
        else if (array[i + 1]>previusNum )
        {
            k = i + 1;
            counter++;
            previusNum=array[i+1];

        }
        
        i++;
        if (i==array.length-1)
        {
            Flag = false;
        }
    }
    for (int z = 0; z <  oldCounter; z++)
    {
        System.out.print(temporaryArray[z]);
    }
}
}
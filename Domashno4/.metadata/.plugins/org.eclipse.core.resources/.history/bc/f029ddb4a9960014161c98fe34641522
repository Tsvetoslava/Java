import java.util.Scanner;


public class Redica1 {
public static void main(String[] args) {
	
	int[] array= new int[10];
	Scanner sc=new Scanner(System.in);
	int counter=1;
	for (int i = 0; i < array.length; i++) {
		System.out.println("Vuvedi " +counter +" chislo: ");
		int number=sc.nextInt();
		array[i] = (number);
		counter++;
}

	print(array);
}


public static void print(int[] array)
{
    int i = 0;
    int previusNum = array[0];
    int k = 0;
    boolean Flag = true;
    int[] temporaryArray = new int[array.length];
    int counter = 0;
    int OldElementCounter = 0;
    while (Flag)
    {

     
        if (array[i + 1]>=array[i])
        {
            previusNum = array[i + 1];
            if (counter > OldElementCounter)
            {
                for (int j = 0; j < counter+1; j++)
                {
                    temporaryArray[j] = array[k -counter];
                    k++;
                }
                OldElementCounter = counter;
            }
            counter = 0;

        }
        else if (array[i + 1]>previusNum )
        {
            k = i + 1;
            counter++;
            previusNum=array[i+1];

        }
        
        i++;
        if (i==array.length-1)
        {
            Flag = false;
        }
    }
    for (int z = 0; z < OldElementCounter+1; z++)
    {
        System.out.print(temporaryArray[z]);
    }
}
}
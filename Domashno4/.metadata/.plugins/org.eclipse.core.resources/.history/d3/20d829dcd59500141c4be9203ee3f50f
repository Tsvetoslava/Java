import java.util.Scanner;


public class SymbolFive {
	public static void main(String[] args) {
	Scanner sc= new Scanner(System.in);
	String g = "torta";
	char cc = g.charAt(0);  
	char[] arr = g.toCharArray(); 
	char [] array={'t','p','o','l','a'};
	
	
	
	for (char c : arr) {
		System.out.print(c);
	}
	
	System.out.println();
	
	for (char c : array) {
		System.out.print(c);
	}
	
	print(arr, array);
	}
	
	public static void print(char[]arr,char[]array){
		for (int i = 0; i < array.length; i++) {
			
		
		if(array[0] == arr[i] || array[1] == arr[i] 
				|| array[2] == arr[i] || array[3] == arr[i] || array[4] == arr[i]){
			System.out.print(" sumvolut suvpada");
		}
		
		else{
			System.err.println("     ne moje da se sustavi duma");
			break;
		}
		}
	}
	
}
	
	


	

	

import java.util.ArrayList;
import java.util.Arrays;


public class StudentController {
public static void main(String[] args) {
	
	Student s1 = new Student();
	
	ArrayList<Integer> gradesPesho = new ArrayList<>(Arrays.asList(5,2,6));
	s1.getOcenki().put("History",gradesPesho);
	s1.getOcenki().put("Sport", new ArrayList<>(Arrays.asList(5,4,6)));
	s1.getOcenki().put("Chemistry", new ArrayList<>(Arrays.asList(6,4,6)));
	s1.getOcenki().put("Physics", new ArrayList<>(Arrays.asList(6,4,6)));
	s1.getOcenki().put("IT", new ArrayList<>(Arrays.asList(6,4,6)));
	
	Student s2 = new Student();
	s2.getOcenki().put("History",new ArrayList<>(Arrays.asList(3,4,6)));
	s2.getOcenki().put("Sport", new ArrayList<>(Arrays.asList(6,4,6)));
	s2.getOcenki().put("Chemistry", new ArrayList<>(Arrays.asList(6,4,3)));
	s2.getOcenki().put("Physics", new ArrayList<>(Arrays.asList(6,4,4)));
	s2.getOcenki().put("IT", new ArrayList<>(Arrays.asList(6,4,5)));
	
	Student s3 = new Student();
	s3.getOcenki().put("History",new ArrayList<>(Arrays.asList(3,4,3)));
	s3.getOcenki().put("Sport", new ArrayList<>(Arrays.asList(6,4,3)));
	s3.getOcenki().put("Chemistry", new ArrayList<>(Arrays.asList(6,4,3)));
	s3.getOcenki().put("Physics", new ArrayList<>(Arrays.asList(6,2,4)));
	s3.getOcenki().put("IT", new ArrayList<>(Arrays.asList(6,6,6)));
	
	for (String key : s1.getOcenki().keySet()) 
	{
		
		System.out.println("" + key + " -> " +s1.getOcenki().get(key));

	}
	
	
	
	
	
}
}

import java.util.ArrayList;
import java.util.Arrays;


public class StudentController {
public static void main(String[] args) {
	
	Student pesho = new Student("Ivan","Ivanov", 56465, null);
	Student kiril = new Student();
	Student krum = new Student();
	ArrayList<Integer> gradesPesho = new ArrayList<>(Arrays.asList(5,2,6));
	pesho.getOcenki().put("History",gradesPesho);
	pesho.getOcenki().put("Sport", new ArrayList<>(Arrays.asList(5,4,6)));
	pesho.getOcenki().put("Chemistry", new ArrayList<>(Arrays.asList(6,4,6)));
	pesho.getOcenki().put("Physics", new ArrayList<>(Arrays.asList(6,4,6)));
	pesho.getOcenki().put("IT", new ArrayList<>(Arrays.asList(6,4,6)));
	

	kiril.getOcenki().put("History",new ArrayList<>(Arrays.asList(3,4,6)));
	kiril.getOcenki().put("Sport", new ArrayList<>(Arrays.asList(6,4,6)));
	kiril.getOcenki().put("Chemistry", new ArrayList<>(Arrays.asList(6,4,3)));
	kiril.getOcenki().put("Physics", new ArrayList<>(Arrays.asList(6,4,4)));
	kiril.getOcenki().put("IT", new ArrayList<>(Arrays.asList(6,4,5)));
	
	krum.getOcenki().put("History",new ArrayList<>(Arrays.asList(3,4,3)));
	krum.getOcenki().put("Sport", new ArrayList<>(Arrays.asList(6,4,3)));
	krum.getOcenki().put("Chemistry", new ArrayList<>(Arrays.asList(6,4,3)));
	krum.getOcenki().put("Physics", new ArrayList<>(Arrays.asList(6,2,4)));
	krum.getOcenki().put("IT", new ArrayList<>(Arrays.asList(6,6,6)));
	
	for (String key : pesho.getOcenki().keySet()) 
	{
		
		System.out.println("" + key + " -> " +pesho.getOcenki().get(key));

	}
	
	
	
	
	
}
}

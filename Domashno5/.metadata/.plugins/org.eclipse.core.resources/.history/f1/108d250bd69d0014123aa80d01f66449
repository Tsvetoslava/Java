import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.HashMap;
import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;


public class TextSort {
	
public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		String words = " ";
		
	
		System.out.println("Type some text to sort alphabetically: ");
		words = scan.nextLine();
		scan.close();
		ArrayList<String> myList = new ArrayList<String>(Arrays.asList(words.split(" ")));
	
			
			System.out.println("Unsorted words : + w");
					for(String w : myList)
		{
			System.out.println(w);
		}
					Collections.sort(myList);	
					HashMap<String,Integer> map = new HashMap<String,Integer>();
				    
					for(String w : myList)
					{
						if(map.get(w)==null)
						{
							map.put(w, 1);
						}
						
						else
						{	
							Integer tmp = map.get(w);
							map.put(w, tmp+1);	
				 		}
						
					}
					System.out.println("Sorted words(No repeating): ");
						Set<String> wordKeys = map.keySet();
							for (String word : wordKeys) 
					{
						int count = map.get(word);
						if(count>=1)
						{
						System.out.printf("%s \n",  word);
						}
			
					}		

}
}


import java.util.HashSet;


public class TownController {
public static void main(String[] args) {
	
	
	
	
	Town t1=new Town("Sofia", "Bulgaiq", 2000000);
	Town t2=new Town("NY", "USA", 20000000);
	Town t3=new Town("Plovdiv", "Bulgaiq", 400000);
	Town t4=new Town("Berlin", "Germany", 3500000);
	Town t5=new Town("Vienna", "Austria", 1700000);
	
	
	HashSet<Town> town = new HashSet<Town>();
	
	
	town.add(t1);
	town.add(t2);
	town.add(t3);
	town.add(t4);
	town.add(t5);
	
	System.out.println(town);
	
	System.out.println(town.contains(t1));
	town.remove(t5);
	
    System.out.println(town);	
}
}


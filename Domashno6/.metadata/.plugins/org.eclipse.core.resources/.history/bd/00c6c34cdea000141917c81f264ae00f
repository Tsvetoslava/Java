
public class Text {


	public static void main(String[] args) {
		
		String text="Az sum ivan.Moeto ime e ivan";
		String word="ivan";
		String sentance="Az sum ivan.";
		
		
		String[] parts = text.split(" |.|!");
		
		
		for (int i = 0; i < parts.length; i++) {
			System.out.println(parts[i]);
		}
	}
}

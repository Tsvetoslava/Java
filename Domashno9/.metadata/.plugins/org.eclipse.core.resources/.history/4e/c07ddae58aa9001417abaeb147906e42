
public class Symbol {
public static void main(String[] args) {
	
	
	
	String izraz="aaaagdfgggggvdfgvffffffgfdg";
	char[]novIzraz=new char[1000];
	
	char[]symbol=izraz.toCharArray();

	
	for (int i = 0; i < symbol.length; i++) {
		if(symbol[i]==i){
			continue;
		}else{
		novIzraz[i]=(char) i;
			
		}
		
		for (char c : novIzraz) {
			System.out.print(c);
		}
	}
	
	
	
	
	
	
	
}
}

/*Ќаправете програма ко€то приема дума от конзолата
 и извежда номера на вс€ка нейна буква
 */

public class Letters {

	public static void main(String[] args) {

		String word = "kola";
		char[] symbol = word.toCharArray();

		for (int i = 0; i < symbol.length; i++) {
			System.out.printf("%s-nomer  %d", symbol[i], i);
			System.out.println();
		}
	}
}
